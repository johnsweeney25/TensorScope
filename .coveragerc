[run]
branch = True
source = .
parallel = True
omit =
    */test_*.py
    */tests/*
    */__pycache__/*
    */venv/*
    */env/*
    */.venv/*
    */site-packages/*
    run_tests.py
    setup.py
    */examples/*
    */.git/*

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain about missing debug-only code:
    def __repr__
    if self\.debug

    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError

    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:
    if TYPE_CHECKING:

    # Don't complain about abstract methods
    @abstract
    @abc.abstractmethod

    # Ignore pass statements
    ^\\s*pass\\s*$

    # Ignore ... (ellipsis) statements
    ^\\s*\\.\\.\\.\\s*$

ignore_errors = True
precision = 1
show_missing = True
skip_covered = False
skip_empty = True

[html]
directory = htmlcov
title = ICLR 2026 Project Coverage Report

[xml]
output = coverage.xml

[json]
output = coverage.json
pretty_print = True